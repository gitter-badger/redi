<urlrewrite>
    <rule>
        <name>Domain Name Check</name>
        <condition name="host" operator="notequal" next="or" >rediclon.cedia.edu.ec</condition>
        <condition type="scheme" operator="equal">https</condition>
        <from>^(.*)$</from>
        <to type="permanent-redirect">http://rediclon.cedia.edu.ec$1</to>
    </rule>
</urlrewrite>
